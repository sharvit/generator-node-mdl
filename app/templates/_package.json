{
  "name": "<%= projectName %>",
  "version": "0.0.0",
  "description": "<%= description %>",
  "license": "MIT",
  "repository": "https://github.com/<%= githubUsername %>/<%= projectName %>",
  "author": {
    "name": "<%= name %>",
    "email": "<%= email %>",
    "url": "<%= website %>"
  },
  "engines": {
    "node": ">=6"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "babel src --out-dir dist --copy-files --ignore **/*.test.js",
    "postbuild": "rimraf dist/**/*.test.js",
    "test:watch": "jest --watch",
    "test": "xo && jest --coverage"
  },
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/core": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-plugin-add-module-exports": "^1.0.0",
    "jest": "^23.6.0",
    "rimraf": "^2.6.2",
    "xo": "^0.23.0"
  },
  "jest": {
    "testEnvironment": "node",
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "collectCoverageFrom": [
      "src/**/*.js"
    ]
  },
  "prettier": {
    "bracketSpacing": true,
    "singleQuote": true
  },
  "xo": {
    "envs": [
      "jest"
    ],
    "prettier": true,
    "space": true
  },
  "keywords": []
}
